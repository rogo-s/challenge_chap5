        <item name="android:minHeight">48dp</item>
        <item name="fontFamily">@font/poppins_regular</item>
    </style>
    <style name="buttonStyle" parent="@style/Widget.Material3.Button.ElevatedButton">
        <item name="android:fontFamily">@font/poppins_semibold</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:backgroundTint">@color/blue_dark</item>
    </style>
    <style name="textViewProfile">
        <item name="android:fontFamily">@font/poppins_regular</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="textViewItemList">
        <item name="android:textSize">12sp</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
        <item name="android:textColor">@color/dark_gray</item>
    </style>
</resources>